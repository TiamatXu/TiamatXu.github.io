{
  "name": "tiamat.xu-docs",
  "author": "TiamatXu",
  "version": "1.0.0",
  "description": "the personal blog of TiamatXu.",
  "license": "MIT License",
  "homepage": "https://tiamatxu.github.io",
  "bugs": {
    "url": "https://github.com/TiamatXu/TiamatXu.github.io/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiamatXu/TiamatXu.github.io.git"
  },
  "keywords": [
    "blog",
    "software",
    "technology"
  ],
  "scripts": {
    "lint": "prettier --check --write \"{.vitepress,docs,vuetheme}/**/*.{ts,vue,css,md}\"",
    "lint:fail": "prettier --check \"{.vitepress,docs,vuetheme}/**/*.{ts,vue,css,md}\"",
    "type": "vue-tsc --noEmit",
    "test": "pnpm run lint && pnpm run type",
    "fetch-contributions": "tsx .github/fetch-contributions.ts",
    "dev": "vitepress dev",
    "build": "vitepress build",
    "serve": "pnpm run build && vitepress serve",
    "preview": "vitepress preview",
    "clean": "rm -rf .vitepress/dist"
  },
  "dependencies": {
    "@docsearch/css": "^3.6.0",
    "@docsearch/js": "^3.6.0",
    "@vueuse/core": "^10.9.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "date-fns": "^4.1.0",
    "markdown-it": "^14.1.1",
    "normalize.css": "^8.0.1",
    "tiny-decode": "^0.1.3"
  },
  "devDependencies": {
    "@mdit-vue/types": "^2.1.0",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/estree": "^1.0.5",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.13",
    "js-yaml": "^4.1.1",
    "node-fetch": "^2.7.0",
    "prettier": "^3.2.5",
    "tsx": "^4.21.0",
    "typescript": "^5.4.5",
    "vitepress": "^1.6.4",
    "vue": "^3.4.27",
    "vue-tsc": "^2.0.19"
  },
  "packageManager": "pnpm@10.27.0"
}
